{
	"name": "@hotcakes/core",
	"version": "1.7.0",
	"main": "./src/index.cjs",
	"types": "./src/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "npm run build:esm && npm run build:cjs",
		"build:esm": "tsc --project tsconfig.esm.json",
		"build:cjs": "tsc --project tsconfig.cjs.json"
	},
	"exports": {
		"./release-config": {
			"import": "./lib/esm/release.config.js",
			"require": "./lib/cjs/release.config.js"
		},
		"./typescript-config": {
			"import": "./lib/esm/typescript.config.json",
			"require": "./lib/cjs/typescript.config.json"
		},
		"./eslint-config-js": {
			"import": "./lib/esm/eslint-js.config.js",
			"require": "./lib/cjs/eslint-js.config.js"
		},
		"./eslint-config-ts": {
			"import": "./lib/esm/eslint-ts.config.js",
			"require": "./lib/cjs/eslint-ts.config.js"
		},
		"./eslint-config-jsx": {
			"import": "./lib/esm/eslint-jsx.config.js",
			"require": "./lib/cjs/eslint-jsx.config.js"
		},
		"./eslint-config-tsx": {
			"import": "./lib/esm/eslint-tsx.config.js",
			"require": "./lib/cjs/eslint-tsx.config.js"
		}
	},
	"files": [
		"lib"
	],
	"repository": "https://github.com/crealgo/hotcakes-core.git",
	"author": "Crealgo, LLC <hello.crealgo@gmail.com> (https://crealgo.com)",
	"license": "ISC",
	"dependencies": {
		"@hotcakes/core": "^1.6.3",
		"@typescript-eslint/eslint-plugin": "^6.3.0",
		"@typescript-eslint/parser": "^6.3.0",
		"eslint-config-typescript": "^3.0.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-config-xo-typescript": "*",
		"eslint-plugin-react": "^7.33.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"lodash": "^4.17.21",
		"xo": "^0.56.0"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.197",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6"
	},
	"peerDependencies": {
		"eslint": "8.x",
		"react": "18.x",
		"react-dom": "18.x",
		"typescript": "5.x"
	}
}
